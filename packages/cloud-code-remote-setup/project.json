{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "cloud-code-remote-setup",
  "projectType": "library",
  "sourceRoot": "packages/cloud-code-remote-setup/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "assets": ["packages/cloud-code-remote-setup/*.md"],
        "main": "packages/cloud-code-remote-setup/src/index.ts",
        "outputPath": "dist/packages/cloud-code-remote-setup",
        "tsConfig": "packages/cloud-code-remote-setup/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/cloud-code-remote-setup/**/*.[jt]s?(x)"],
        "maxWarnings": 0
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "packages/cloud-code-remote-setup/jest.config.ts"
      }
    }
  }
}
