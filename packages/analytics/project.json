{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "analytics",
  "projectType": "library",
  "sourceRoot": "packages/analytics/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "assets": ["packages/analytics/*.md"],
        "main": "packages/analytics/src/index.js",
        "outputPath": "dist/packages/analytics",
        "tsConfig": "packages/analytics/tsconfig.lib.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/analytics/**/*.[jt]s?(x)"],
        "maxWarnings": 0
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "packages/analytics/jest.config.ts"
      }
    }
  }
}
