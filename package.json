{
  "name": "@clipboard-health/core-utils",
  "dependencies": {
    "tslib": "2.7.0"
  },
  "devDependencies": {
    "@nx/eslint": "20.0.0",
    "@nx/eslint-plugin": "20.0.0",
    "@nx/jest": "20.0.0",
    "@nx/js": "20.0.0",
    "@nx/plugin": "20.0.0",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.26",
    "@swc/helpers": "0.5.13",
    "@tsconfig/node22": "22.0.0",
    "@tsconfig/strictest": "2.0.5",
    "@types/jest": "29.5.13",
    "@types/node": "22.7.5",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cspell": "8.14.4",
    "embedme": "1.22.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-xo": "0.45.0",
    "eslint-config-xo-react": "0.27.0",
    "eslint-config-xo-typescript": "1.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-expect-type": "0.4.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.7.0",
    "eslint-plugin-n": "16.3.0",
    "eslint-plugin-no-only-tests": "3.1.0",
    "eslint-plugin-no-use-extend-native": "0.5.0",
    "eslint-plugin-react": "7.35.2",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lint-staged": "15.2.10",
    "nx": "20.0.0",
    "prettier": "3.3.3",
    "syncpack": "13.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "type-fest": "4.26.1",
    "typedoc": "0.26.8",
    "typescript": "5.6.3"
  },
  "license": "MIT",
  "overrides": {
    "tsconfig-paths": "4.2.0"
  },
  "private": true,
  "scripts": {
    "affected": "nx affected --base main --configuration ci --parallel 8 --targets build,lint,test",
    "all": "nx run-many --configuration ci --parallel 8 --targets build,lint,test",
    "ci:check": "nx format:check && prettier --check . && npm run cspell -- . && syncpack lint && npm run embed:check",
    "ci:install": "npm install --audit false --fund false",
    "cspell": "npx cspell --no-summary --no-progress --no-must-find-files",
    "docs": "rm -rf docs && typedoc",
    "embed": "nx run-many --targets embed",
    "embed:check": "npm run embed -- --verify",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "workspaces": [
    "packages/*"
  ]
}
