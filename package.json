{
  "name": "@clipboard-health/core-utils",
  "version": "0.0.0",
  "dependencies": {
    "tslib": "2.6.3"
  },
  "devDependencies": {
    "@nx/devkit": "19.5.6",
    "@nx/eslint": "19.5.6",
    "@nx/eslint-plugin": "19.5.6",
    "@nx/jest": "19.5.6",
    "@nx/js": "19.5.6",
    "@nx/plugin": "19.5.6",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.6",
    "@swc/helpers": "0.5.12",
    "@tsconfig/node22": "22.0.0",
    "@tsconfig/strictest": "2.0.5",
    "@types/jest": "29.5.12",
    "@types/node": "22.1.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cspell": "8.13.1",
    "embedme": "1.22.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lint-staged": "15.2.8",
    "nx": "19.5.6",
    "prettier": "3.3.3",
    "syncpack": "12.4.0",
    "ts-jest": "29.2.4",
    "ts-node": "10.9.2",
    "typedoc": "0.26.5",
    "typescript": "5.5.4"
  },
  "license": "MIT",
  "overrides": {
    "tsconfig-paths": "4.2.0"
  },
  "private": true,
  "scripts": {
    "affected": "nx affected --base main --configuration ci --parallel 8 --targets build,lint,test",
    "all": "nx run-many --configuration ci --parallel 8 --targets build,lint,test",
    "check:ci": "nx format:check && prettier --check . && npm run cspell -- . && syncpack lint",
    "cspell": "npx cspell --no-summary --no-progress --no-must-find-files",
    "docs": "rm -rf docs && typedoc",
    "format": "prettier --write .",
    "install:ci": "npm clean-install --audit false --fund false",
    "prepare": "husky"
  },
  "workspaces": [
    "packages/*"
  ]
}
